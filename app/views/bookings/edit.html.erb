
<div class="container">
  <h2>Edit your booking</h2>
  <div class="row">
    <div class=".col-md-3 .offset-md-3">
      <div class="form-box">
        <%= simple_form_for [@space, @booking] do |f| %>
        <%= f.input :date %>
        <%= f.submit "Edit your booking", class: "btn btn-primary" %>
        <% end  %>
      </div>
    </div>
  </div>
</div>
